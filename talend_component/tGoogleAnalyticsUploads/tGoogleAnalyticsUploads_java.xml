<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT>
	<HEADER 
	    PLATEFORM="ALL" 
	    SERIAL="" 
	    VERSION="1.1" 
	    STATUS="PROD"
		COMPATIBILITY="ALL" 
		AUTHOR="Jan Lolling" 
		RELEASE_DATE="20151218A"
		HAS_CONDITIONAL_OUTPUTS="true"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Business/Google</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
		<CONNECTOR NAME="CURRENT_UPLOAD" CTYPE="FLOW" MAX_INPUT="0"
			MAX_OUTPUT="1" LINE_STYLE="1" SHOW_IF="MODE == 'UPLOAD'"/>
		<CONNECTOR NAME="UPLOADS" CTYPE="FLOW" MAX_INPUT="0"
			MAX_OUTPUT="1" LINE_STYLE="1" SHOW_IF="MODE == 'LIST_UPLOADS'"/>
		<CONNECTOR NAME="CUSTOM_DATA_SOURCES" CTYPE="FLOW" MAX_INPUT="0"
			MAX_OUTPUT="1" LINE_STYLE="1" SHOW_IF="MODE == 'LIST_CUSTOM_DATA_SOURCES'" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER FIELD="CLOSED_LIST" NAME="MODE" NUM_ROW="1" REQUIRED="true" >
    	<ITEMS DEFAULT="UPLOAD">
    		<ITEM NAME="UPLOAD" VALUE="UPLOAD" />
    		<ITEM NAME="LIST_UPLOADS" VALUE="LIST_UPLOADS" />
    		<ITEM NAME="LIST_CUSTOM_DATA_SOURCES" VALUE="LIST_CUSTOM_DATA_SOURCES" />
    	</ITEMS>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="APPLICATION_NAME" NUM_ROW="10"
			GROUP="CLIENT">
			<DEFAULT>"Manage Uploads Talend Component"</DEFAULT>
		</PARAMETER>
		<PARAMETER 
			FIELD="CLOSED_LIST" 
			NAME="AUTHENTICATION_METHOD" 
			NUM_ROW="15" 
			REQUIRED="true" 
			GROUP="CLIENT">
	    	<ITEMS DEFAULT="SERVICE_ACCOUNT">
	    		<ITEM NAME="SERVICE_ACCOUNT" VALUE="SERVICE_ACCOUNT" />
	    		<ITEM NAME="APPLICATION_CLIENT_ID" VALUE="APPLICATION_CLIENT_ID" />
	    	</ITEMS>
		</PARAMETER>
		<PARAMETER 
			FIELD="TEXT" 
			NAME="SERVICE_ACCOUNT_EMAIL" 
			NUM_ROW="20" 
			REQUIRED="true" 
			GROUP="CLIENT" 
			SHOW_IF="(AUTHENTICATION_METHOD == 'SERVICE_ACCOUNT')">
			<DEFAULT>"[service-account-id]developer.gserviceaccount.com"</DEFAULT>
		</PARAMETER>
		<PARAMETER 
			FIELD="FILE" 
			NAME="KEY_FILE" 
			NUM_ROW="21" 
			REQUIRED="true" 
			GROUP="CLIENT" 
			SHOW_IF="(AUTHENTICATION_METHOD == 'SERVICE_ACCOUNT')">
			<DEFAULT>"/tmp/76548c6bc96518473d7e425e1415a14e495d34eb-privatekey.p12"</DEFAULT>
		</PARAMETER>
		<PARAMETER 
			FIELD="TEXT" 
			NAME="USER_ACCOUNT_EMAIL" 
			NUM_ROW="20" 
			REQUIRED="true" 
			GROUP="CLIENT" 
			SHOW_IF="(AUTHENTICATION_METHOD == 'APPLICATION_CLIENT_ID')">
			<DEFAULT>"yourmail@gmail.com"</DEFAULT>
		</PARAMETER>
		<PARAMETER 
			FIELD="FILE" 
			NAME="CLIENT_SECRET_FILE" 
			NUM_ROW="21" 
			REQUIRED="true" 
			GROUP="CLIENT" 
			SHOW_IF="(AUTHENTICATION_METHOD == 'APPLICATION_CLIENT_ID')">
			<DEFAULT>"/tmp/client_secret.json"</DEFAULT>
		</PARAMETER>

		<PARAMETER FIELD="TEXT" NAME="ACCOUNT_ID" NUM_ROW="50" REQUIRED="true" >
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="WEBPROPERTY_ID" NUM_ROW="60" REQUIRED="true" >
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="CUSTOM_DATA_SOURCE_ID" NUM_ROW="70" REQUIRED="true" SHOW_IF="(MODE == 'UPLOAD') or (MODE == 'LIST_UPLOADS')">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="FILE" NAME="UPLOAD_FILE" NUM_ROW="80" REQUIRED="true" SHOW_IF="MODE == 'UPLOAD'">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_CUSTOM_DATA_SOURCES" FIELD="SCHEMA_TYPE"
			REQUIRED="true" NUM_ROW="200" CONTEXT="CUSTOM_DATA_SOURCES" SHOW_IF="MODE == 'LIST_CUSTOM_DATA_SOURCES'">
			<TABLE READONLY="true">
				<COLUMN NAME="CDS_ID" TYPE="id_String" KEY="true" COMMENT="ID of the custom data source (CDS)"
					READONLY="true" CUSTOM="true" LENGTH="64" NULLABLE="false" />
				<COLUMN NAME="ACCOUNT_ID" TYPE="id_Long" READONLY="true" COMMENT="Account ID"
					CUSTOM="true" LENGTH="22" NULLABLE="false" />
				<COLUMN NAME="WEBPROPERTY_ID" TYPE="id_String" READONLY="true" COMMENT="Web Property ID"
					CUSTOM="true" LENGTH="32" NULLABLE="false" />
				<COLUMN NAME="CDS_NAME" TYPE="id_String" READONLY="true" COMMENT="Name of the custome data source"
					CUSTOM="true" LENGTH="64" NULLABLE="true" />
				<COLUMN NAME="CDS_TYPE" TYPE="id_String" READONLY="true" COMMENT="Type like COST or REFUND etc."
					CUSTOM="true" LENGTH="64" NULLABLE="true" />
				<COLUMN NAME="CDS_IMPORT_BEHAVIOR" TYPE="id_String" READONLY="true" COMMENT="How to handle the uploads. Example: OVERWRITE"
					CUSTOM="true" LENGTH="64" NULLABLE="true" />
				<COLUMN NAME="CDS_LINKED_PROFILES" TYPE="id_String" READONLY="true" COMMENT="Comma separated list of the linked views"
					CUSTOM="true" LENGTH="512" NULLABLE="true" />
				<COLUMN NAME="CDS_CREATED" TYPE="id_Date" READONLY="true"
					CUSTOM="true" PATTERN="&quot;yyyy-MM-dd HH:mm:ss&quot;" LENGTH="10" NULLABLE="false"/>
				<COLUMN NAME="CDS_UPDATED" TYPE="id_Date" READONLY="true"
					CUSTOM="true" PATTERN="&quot;yyyy-MM-dd HH:mm:ss&quot;" LENGTH="10"/>
			</TABLE>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_UPLOADS" FIELD="SCHEMA_TYPE"
			REQUIRED="true" NUM_ROW="200" CONTEXT="UPLOADS" SHOW_IF="MODE == 'LIST_UPLOADS'">
			<TABLE READONLY="true">
				<COLUMN NAME="UPLOAD_ID" TYPE="id_String" READONLY="true"
					CUSTOM="true" LENGTH="32" KEY="true" NULLABLE="false" COMMENT="ID of the unsampled report" />
				<COLUMN NAME="UPLOAD_STATUS" TYPE="id_String" READONLY="true" COMMENT="Status of the upload"
					CUSTOM="true" LENGTH="22" NULLABLE="false" />
				<COLUMN NAME="ACCOUNT_ID" TYPE="id_Long" READONLY="true" COMMENT="Account ID to which this upload belongs."
					CUSTOM="true" LENGTH="22" NULLABLE="false" />
				<COLUMN NAME="UPLOAD_ERRORS" TYPE="id_String" READONLY="true"
					CUSTOM="true" LENGTH="1024" NULLABLE="true"/>
				<COLUMN NAME="CDS_ID" TYPE="id_String" COMMENT="ID of the custom data source (CDS)"
					READONLY="true" CUSTOM="true" LENGTH="64" NULLABLE="false" />
			</TABLE>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_CURRENT_UPLOAD" FIELD="SCHEMA_TYPE"
			REQUIRED="true" NUM_ROW="200" CONTEXT="CURRENT_UPLOAD" SHOW_IF="MODE == 'UPLOAD'">
			<TABLE READONLY="true">
				<COLUMN NAME="UPLOAD_ID" TYPE="id_String" READONLY="true"
					CUSTOM="true" LENGTH="32" KEY="true" NULLABLE="false" COMMENT="ID of the unsampled report" />
				<COLUMN NAME="UPLOAD_STATUS" TYPE="id_String" READONLY="true" COMMENT="Status of the upload"
					CUSTOM="true" LENGTH="22" NULLABLE="false" />
				<COLUMN NAME="ACCOUNT_ID" TYPE="id_Long" READONLY="true" COMMENT="Account ID to which this upload belongs."
					CUSTOM="true" LENGTH="22" NULLABLE="false" />
				<COLUMN NAME="UPLOAD_ERRORS" TYPE="id_String" READONLY="true"
					CUSTOM="true" LENGTH="1024" NULLABLE="true"/>
				<COLUMN NAME="CDS_ID" TYPE="id_String" COMMENT="ID of the custom data source (CDS)"
					READONLY="true" CUSTOM="true" LENGTH="64" NULLABLE="false" />
				<COLUMN NAME="LOCAL_FILEPATH" TYPE="id_String" COMMENT="Local file uploaded"
					READONLY="true" CUSTOM="true" LENGTH="128" NULLABLE="false" />
			</TABLE>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
		<PARAMETER FIELD="TEXT" NAME="TIMEOUT_IN_SEC" NUM_ROW="10">
			<DEFAULT>240</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="TIME_OFFSET" NUM_ROW="15">
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="KEEP_CLIENT" NUM_ROW="40">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="KEEP_CLIENT_KEY" NUM_ROW="40"
			SHOW_IF="KEEP_CLIENT == 'true'">
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Google Analytics Uploads"
				MODULE="jlo.talendcomp.gauploads-1.1-jar-with-dependencies.jar"
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="UPLOADS_NB_LINE" TYPE="id_Integer"/>
		<RETURN AVAILABILITY="AFTER" NAME="CUSTOM_DATA_SOURCES_NB_LINE" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="UPLOAD_FILEPATH" TYPE="id_String" />
	</RETURNS>
</COMPONENT>
